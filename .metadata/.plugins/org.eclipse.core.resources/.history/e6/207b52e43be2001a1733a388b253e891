package entidade;

public class Empregado {

	public String name;
	public double grossSalary;
	public double tax;
	public double percentage;
	
	public void increaseSalary() {
		grossSalary = grossSalary + (grossSalary * percentage / 100);
	}
	public double netSalary() {
		return grossSalary - tax;
	}
}

