package curso_programacao;

import java.util.Scanner;

public class Altura {

	public static void main(String[] args) {
		
		Scanner teclado = new Scanner(System.in);
		
		int n = teclado.nextInt();
		double [] vect = new double [n];
		
		for (int i = 0; i < n; i++) {
			vect[i] = teclado.nextDouble();
		}
		
		teclado.close();
	}

}
