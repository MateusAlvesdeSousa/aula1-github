import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

		Scanner teclado = new Scanner(System.in);
		
		int numero = teclado.nextInt();
		int soma = 0;
		
		while (numero != 0) {
			soma = soma + numero;
			numero = teclado.nextInt();
			System.out.println(soma);
		}
		
		teclado.close();
	}
}